## What happens if multiple processes write to the same file?
## To submit:  condor_submit -append "exercise = 4.n" <submit file>  

universe = vanilla

executable = /bin/echo
args = Hello World from job $(CLUSTER).$(PROCESS) executing exercise $(EXERCISE).

output = hello.exercise$(EXERCISE).$(CLUSTER).out
error  = hello.exercise$(EXERCISE).$(CLUSTER).err
log    = hello.exercise$(EXERCISE).$(CLUSTER).log

queue 5
